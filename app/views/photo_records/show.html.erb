<div class="page-header">
  <%= link_to photo_records_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_photo_record_path(@photo_record), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show photo_record</h1>
</div>

<dl class="dl-horizontal">
  <dt>Event:</dt>
  <dd><%= @photo_record.event %></dd>

  <dt>User:</dt>
  <dd><%= @photo_record.user %></dd>

</dl>
